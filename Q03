package com.mycompany.p05q03;

public abstract class Student 
{
    final int marks=100;
    final void display()
    {
        System.out.println("Marks:"+marks);
    }
    
}

package com.mycompany.p05q03;

public class Undergraduate extends Student
{
    
}

package com.mycompany.p05q03;

public class P05Q03
{

    public static void main(String[] args)
    {
       Undergraduate U1=new Undergraduate();
       U1.display();
    }
}
